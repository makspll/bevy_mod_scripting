{
    "lldb.displayFormat": "auto",
    "lldb.showDisassembly": "never",
    "lldb.dereferencePointers": true,
    "lldb.consoleMode": "commands",

    "rust-analyzer.cargo.features": [
        "lua54",
        "lua_script_api",
        "rhai",
        "rhai_script_api",
        "teal"
    ],
    "rust-analyzer.server.extraEnv": {
        "RUSTUP_TOOLCHAIN": "stable"
    }
}

